{
  "name": "vscode-breadcrumbs-alt",
  "displayName": "Breadcrumbs view",
  "icon": "images/icon.png",
  "description": "Breadcrumbs view based on VSCode Webview",
  "version": "1.0.40",
  "license": "MIT",
  "preview": true,
  "main": "./dist/ext-src/extension.js",
  "publisher": "sandipchitale",
  "author": {
    "name": "Sandip Chitale"
  },
  "maintainers": [
      "Sandip Chitale <sandipchitale@gmail.com>"
  ],
  "repository": {
      "type": "git",
      "url": "https://github.com/sandipchitale/vscode-breadcrumbs-alt"
  },
  "contributes": {
    "configuration": {
      "properties": {
        "vscode-breadcrumbs-alt.breadcrumbFromWorkspaceRoot": {
          "type": "boolean",
          "default": true,
          "description": "Show breadcrumbs from workspace root."
        },
        "vscode-breadcrumbs-alt.showBreadcrumbIcons": {
          "type": "boolean",
          "default": true,
          "description": "Show icons in breadcrumbs."
        }
      }
    },
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "vscode-breadcrumbs-alt.breadcrumbs",
          "name": "Breadcrumbs"
        }
      ]
    }
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
		"Other"
	],
  "activationEvents": [
    "onView:vscode-breadcrumbs-alt.breadcrumbs"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --output-hashing none && tsc -p tsconfig.extension.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "vsce package --yarn",
    "vscode:prepublish": "npm run build && tsc -p tsconfig.extension.json"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "13.0.1",
    "@angular/cdk": "^13.0.1",
    "@angular/common": "13.0.1",
    "@angular/compiler": "13.0.1",
    "@angular/core": "13.0.1",
    "@angular/forms": "13.0.1",
    "@angular/platform-browser": "13.0.1",
    "@angular/platform-browser-dynamic": "13.0.1",
    "@angular/router": "13.0.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "primeflex": "^2.0.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.0.4",
    "quill": "1.3.7",
    "rxjs": "7.4.0",
    "tslib": "^2.1.0",
    "vscode-test": "^1.5.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.0.0",
    "@angular-devkit/build-angular": "13.0.2",
    "@angular-eslint/builder": "13.0.0-alpha.0",
    "@angular-eslint/eslint-plugin": "13.0.0-alpha.0",
    "@angular-eslint/eslint-plugin-template": "13.0.0-alpha.0",
    "@angular-eslint/schematics": "13.0.0-alpha.0",
    "@angular-eslint/template-parser": "13.0.0-alpha.0",
    "@angular/language-service": "13.0.1",
    "@angular/cli": "13.0.2",
    "@angular/compiler-cli": "13.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/glob": "^7.1.3",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "^4.3.2 <4.5.0",
    "vsce": "^2.5.3"
  }
}
